<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="1" name="HighLineWarren Sanity Suite" parallel="tests">
<listeners>
<listener class-name="utils.SuiteListener"></listener>
</listeners>
  <test name="ChromeTest">
  <parameter name="browser" value="chrome"></parameter>
  <parameter name="resolution" value="1920x1080"></parameter>
      <classes>
      <class name="highlineWarren.test.HighLineWarrenDemoTest"/>
      <class name="highlineWarren.test.ValidateHighLineWarrenDOMTest"/>
      
    </classes>
  </test> <!-- Test -->
   <test name="FireFoxTest">
  <parameter name="browser" value="firefox"></parameter>
  <parameter name="resolution" value="1366x768"></parameter>
      <classes>
      <class name="highlineWarren.test.HighLineWarrenDemoTest"/>
      <class name="highlineWarren.test.ValidateHighLineWarrenDOMTest"/>
    </classes>
  </test> <!-- Test -->
   <test name="EdgeTest">
  <parameter name="browser" value="edge"></parameter>
  <parameter name="resolution" value="1280x800"></parameter>
      <classes>
      <class name="highlineWarren.test.HighLineWarrenDemoTest"/>
      <class name="highlineWarren.test.ValidateHighLineWarrenDOMTest"/>
    </classes>
  </test> <!-- Test -->
  
  </suite> <!-- Suite -->
